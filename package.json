    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vant": "^4.8.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "xgplayer": "^3.0.12",
    "xgplayer-hls.js": "^3.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
